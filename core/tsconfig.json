{
   "compilerOptions": {
      "lib": [ "es6", "dom"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": false,
      "module": "CommonJS",
      "noEmitOnError": true,
      "removeComments": false,
      "sourceMap": true,
      "target": "es6",
      "jsx": "react"
   },
   "exclude": [
      "node_modules"
   ],
   "files": [
      "dev/EntryPoints.tsx"
   ]
}
